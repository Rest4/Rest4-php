title=Deleting the table
request.method=DELETE
request.uri=/db/sample/sampleTable.dat
request.headers.!.name=Accept
request.headers.*.value=text/javascript, text/html, application/xml, text/xml, */*
request.headers.+.name=Accept-Charset
request.headers.*.value=ISO-8859-1,utf-8;q=0.7,*;q=0.3
request.headers.+.name=Content-Type
request.headers.*.value=text/varstream
response.code=410
response.headers.!.name=Content-Type
response.headers.*.value=text/varstream
