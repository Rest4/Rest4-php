# Main
server.protocol=https
server.charset=UTF-8
server.webmaster=webmaster@elitwork.com
server.auth=digest
server.realm=XRest
server.defaultLang=en
server.defaultLocale=US
# Http
http.cache=true
http.maxage=0
http.revalidate=true
http.gzip=4000
# Unix
unix.user=apache
unix.group=apache
unix.path=/var/www
# Database
database.bin=/usr/bin
database.type=mysql
database.host=localhost
